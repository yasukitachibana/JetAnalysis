          <Item>8</Item>
          <Item>9</Item>
          <Item>10</Item>
          <Item>11</Item>
          <Item>13</Item>
          <Item>16</Item>
        </mG4>

        <mGOverPt>
          <Item>0.0</Item>
          <Item>1.0</Item>
        </mGOverPt>

        <ktG>
          <Item>0.0</Item>
          <Item>100.0</Item>
        </ktG>


      </var>
    </SoftDropGroom>

  </observable>

  <inputFiles>
    <inputStyle>JetScapeAscii</inputStyle>
    <events>
      <head>JetscapeHadronListBin</head>
      <join>_</join>
      <tail>.out</tail>
    </events>
    <sigma>
