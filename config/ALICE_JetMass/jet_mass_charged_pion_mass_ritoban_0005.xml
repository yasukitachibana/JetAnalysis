          <Item>9</Item>
          <Item>10</Item>
          <Item>11</Item>
          <Item>13</Item>
          <Item>16</Item>
        </m4>

      </var>
    </JetMass>

  </observable>

  <inputFiles>
    <inputStyle>JetScapeAsciiV2OneLine</inputStyle>
    <sigmaLastLine>1</sigmaLastLine>
    <events>
      <head>JetscapeHadronListBin</head>
      <join>_</join>
      <tail>.out</tail>
    </events>
    <divNum>-1</divNum> <!-- 0: default, 1-:  divided. -1: vector-->
    <divUnit>_Run</divUnit>
    <sigma>
      <pGun>0</pGun>
    </sigma>
  </inputFiles>


