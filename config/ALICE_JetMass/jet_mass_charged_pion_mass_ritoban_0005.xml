<?xml version="1.0"?>

<jetAnalysis>

  <mode>0</mode>  <!-- 0: w/ combine, 1: w/o combine, 2: only combine-->
  <subtractionMethod>NaiveNegativeSub</subtractionMethod>

  <!-- Jet Reconstruction-->
  <jetReco>
    <reconstructionMethod>NegativeReco</reconstructionMethod>
    <jetR>
      <Item>0.2</Item>
    </jetR>

    <chJet>1</chJet>    <!-- 0: full jet, 1: charged jet -->

    <jetRapidity>1</jetRapidity>    <!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <jetRapMin>
      <Item>0</Item>
    </jetRapMin>
    <jetRapMax>
      <Item>0.7</Item>
    </jetRapMax>

    <!-- For ALICE Unfolded resutls -->
    <jetConstPtMin>0.0</jetConstPtMin>
    <jetConstPtMax>2510.0</jetConstPtMax>

    <jetPtMin>
      <Item>40</Item>
      <Item>60</Item>
      <Item>80</Item>
      <Item>100</Item>
    </jetPtMin>
    <jetPtMax>
      <Item>60</Item>
      <Item>80</Item>
      <Item>100</Item>
      <Item>150</Item>
    </jetPtMax>

    <nJetEv>0</nJetEv>    <!-- 0: inclusive -->
  </jetReco>

  <!-- Set Particle Mass -->
  <massAdjust>
    <adjust>1</adjust>
    <adjustedQuant>E</adjustedQuant>    <!-- MOM, Mom, mom, MOMENTUM, Momentum, momentum, P, p: momentum
    ENE, Ene, ene, ENERGY, Energy, energy, E, e: energy -->
    <mass>-211</mass>    <!-- 0 or positive value: mass in GeV, negative value: particle id-->
  </massAdjust>


  <!-- Observable -->
  <observable>

    <JetMass>
      <name>Jet Mass</name>
      <chParticle>1</chParticle>

      <particleRapidity>1</particleRapidity>
      <particleRapMin>
        <Item>0.0</Item>
      </particleRapMin>
      <particleRapMax>
        <Item>2.0</Item>
      </particleRapMax>

      <!-- For ALICE Unfolded resutls -->
      <particlePtMin>
        <Item>0.0</Item>
      </particlePtMin>
      <particlePtMax>
        <Item>2510.0</Item>
      </particlePtMax>

      <var>

        <m>
          <Item>0</Item>
          <Item>1.2</Item>
          <Item>1.6</Item>
          <Item>2</Item>
          <Item>2.4</Item>
          <Item>2.8</Item>
          <Item>3.2</Item>
          <Item>3.6</Item>
          <Item>4</Item>
          <Item>4.4</Item>
          <Item>5</Item>
          <Item>5.6</Item>
          <Item>6.2</Item>
          <Item>7</Item>
        </m>

        <m2>
          <Item>0</Item>
