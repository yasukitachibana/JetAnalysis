<?xml version="1.0"?>

<jetAnalysis>

  <mode>0</mode>  <!-- 0: w/ combine, 1: w/o combine, 2: only combine-->
  <subtractionMethod>NaiveNegativeSub</subtractionMethod>

  <!-- Jet Reconstruction-->
  <jetReco>
    <reconstructionMethod>NegativeReco</reconstructionMethod>
    <jetR>
      <Item>0.2</Item>
      <Item>0.4</Item>
    </jetR>

    <chJet>1</chJet>    <!-- 0: full jet, 1: charged jet -->

    <jetRapidity>1</jetRapidity>    <!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <jetRapMin>
      <Item>0</Item>
      <Item>0</Item>
    </jetRapMin>
    <jetRapMax>
      <Item>0.5</Item>
      <Item>0.7</Item>
    </jetRapMax>

    <jetConstPtMin>0.15</jetConstPtMin>
    <jetConstPtMax>100</jetConstPtMax>

    <jetPtMin>
      <Item>60</Item>
      <Item>80</Item>
    </jetPtMin>
    <jetPtMax>
      <Item>80</Item>
      <Item>100</Item>
    </jetPtMax>
    <nJetEv>0</nJetEv>    <!-- 0: inclusive -->
  </jetReco>

  <observable>

    <SoftDropGroom>
      <name>Soft Drop Grooming</name>
      <chParticle>1</chParticle>
      <!-- <statParticle>
        <Item>0</Item>
        <Item>1</Item>
        <Item>-1</Item>
      </statParticle> -->

      <particleRapidity>1</particleRapidity>
      
      <particleRapMin>
        <Item>0.0</Item>
      </particleRapMin>
      <particleRapMax>
        <Item>0.9</Item>
      </particleRapMax>

      <particlePtMin>
        <Item>0.0</Item>
      </particlePtMin>
      <particlePtMax>
        <Item>100</Item>
      </particlePtMax>

      <beta>
        <Item>0.0</Item>
      </beta>
      <zCut>
        <Item>0.2</Item>
        <Item>0.4</Item>
      </zCut>

      <var>

        <zG>
          <Item>0.2</Item>
          <Item>0.25</Item>
          <Item>0.3</Item>
          <Item>0.35</Item>
          <Item>0.4</Item>
          <Item>0.45</Item>          
          <Item>0.5</Item>
        </zG>

        <thetaG>
          <Item>0</Item>
          <Item>0.1</Item>
          <Item>0.15</Item>
          <Item>0.2</Item>
          <Item>0.25</Item>
          <Item>0.3</Item>
          <Item>0.4</Item>
          <Item>0.5</Item>
          <Item>0.7</Item>
          <Item>1.0</Item>
        </thetaG>

      </var>

    </SoftDropGroom>

  </observable>

  <inputFiles>
    <inputStyle>JetScapeAscii</inputStyle>
    <events>
      <head>JetscapeHadronListBin</head>
      <join>_</join>
      <tail>.out</tail>
    </events>
    <sigma>
      <path>/wsu/home/go/go54/go5410/maj-shen/AAPaperData/5020_PP_Colorless</path>
      <head>SigmaHardBin</head>
      <join>_</join>
      <tail>.out</tail>
    </sigma>
    <divNum>0</divNum>    <!-- 0: default, 1-:  divided-->
    <divUnit>_Run</divUnit>
  </inputFiles>

