<?xml version="1.0"?>

<jetAnalysis>

  <mode>0</mode>  <!-- 0: w/ combine, 1: w/o combine, 2: only combine-->
  <subtractionMethod>NaiveNegativeSub</subtractionMethod>

  <!-- Jet Reconstruction-->
  <jetReco>
    <reconstructionMethod>NegativeReco</reconstructionMethod>
    <jetR>
      <Item>0.4</Item>
    </jetR>

    <chJet>0</chJet>    <!-- 0: full jet, 1: charged jet -->

    <jetRapidity>1</jetRapidity>    <!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <jetRapMin>
      <Item>0</Item>
    </jetRapMin>
    <jetRapMax>
      <Item>2.0</Item>
    </jetRapMax>

    <jetConstPtMin>0.0</jetConstPtMin>
    <jetConstPtMax>10000.0</jetConstPtMax>

    <jetPtMin>
      <Item>0.</Item>
      <Item>0.2</Item>
      <Item>0.3</Item>
      <Item>0.4</Item>
      <Item>0.5</Item>
      <Item>0.6</Item>
      <Item>0.7</Item>
      <Item>0.8</Item>
      <Item>0.9</Item>
      <Item>1.0</Item>
    </jetPtMin>
    <jetPtMax>
      <Item>10</Item>
      <Item>10</Item>
      <Item>10</Item>
      <Item>10</Item>
      <Item>10</Item>
      <Item>10</Item>
      <Item>10</Item>
      <Item>10</Item>
      <Item>10</Item>
      <Item>10</Item>
    </jetPtMax>
    <nJetEv>0</nJetEv>    <!-- 0: inclusive -->
  </jetReco>

  <jetTag>
    <tagged>1</tagged>
    <xJetTag>1</xJetTag>    <!-- 1: jet triggered by momentum fraction Xjtag instead of pTjet-->
    <nTagEv>0</nTagEv>    <!-- 0: inclusive, 1: Highest pt in each event -->
    <!-- tag basic iinfo -->
    <tagParticle>
      <Item>22</Item>
    </tagParticle>
    <tagStat>
      <Item>0</Item>
      <Item>1</Item>
    </tagStat>
    <!-- tag pt cut -->
    <tagPtMin>150</tagPtMin>
    <tagPtMax>200</tagPtMax>
    <!-- tag rapidity cut -->
    <tagRapidity>1</tagRapidity>    <!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <tagRapMin>0</tagRapMin>
    <tagRapMax>2.</tagRapMax>
    <!-- tag isolation -->
    <isolation>1</isolation>
    <RIsolation>0.3</RIsolation>
    <cutIsolation>3</cutIsolation>
    <varIsolation>ET</varIsolation>    <!-- pT, PT, Pt, pt: transverse momentum   ET, et, Et, eT: transverse energy -->
    <!-- <chIsolation>1</chIsolation>   -->
    <statIsolation>
      <Item>0</Item>
      <Item>1</Item>
      <Item>-1</Item>
    </statIsolation>
    <!-- tag relative angle -->
    <deltaPhiCut>1</deltaPhiCut>
    <deltaPhiMin>-0.875</deltaPhiMin>    <!-- negative value: to be multiplied by pi, 7/8 = 0.875-->
  </jetTag>

  <!-- Observable -->
  <observable>

    <SoftDropGroom>
      <name>Soft Drop Grooming</name>
      <chParticle>0</chParticle>
      <!-- <statParticle>
        <Item>0</Item>
        <Item>1</Item>
        <Item>-1</Item>
      </statParticle> -->

      <particleRapidity>1</particleRapidity>

      <particleRapMin>
        <Item>0.0</Item>
      </particleRapMin>
      <particleRapMax>
        <Item>3.0</Item>
      </particleRapMax>

      <!-- For ALICE Unfolded resutls -->
      <particlePtMin>
        <Item>0.0</Item>
      </particlePtMin>
      <particlePtMax>
        <Item>10000.0</Item>
      </particlePtMax>

      <beta>
        <Item>0.0</Item>
      </beta>
      <zCut>
        <Item>0.2</Item>
      </zCut>

      <var>

        <zG>
          <Item>0.2</Item>
          <Item>0.25</Item>
          <Item>0.3</Item>
          <Item>0.35</Item>
          <Item>0.4</Item>
          <Item>0.45</Item>
          <Item>0.5</Item>
        </zG>

        <thetaG>
          <Item>0</Item>
          <Item>1000</Item>
        </thetaG>

        <rG>
          <Item>0</Item>
          <Item>0.003</Item>
          <Item>0.0042</Item>
          <Item>0.0063</Item>
          <Item>0.0096</Item>
          <Item>0.0145</Item>
          <Item>0.0220</Item>
          <Item>0.0332</Item>
          <Item>0.0503</Item>
          <Item>0.0761</Item>
          <Item>0.1153</Item>
          <Item>0.1745</Item>
          <Item>0.2642</Item>
          <Item>0.4</Item>
        </rG>

        <mG>
          <Item>0</Item>
          <Item>5000</Item>
        </mG>

        <mGOverPt>
          <Item>0</Item>
          <Item>0.04</Item>
          <Item>0.06</Item>
          <Item>0.08</Item>
          <Item>0.1</Item>
          <Item>0.12</Item>
          <Item>0.15</Item>
          <Item>0.18</Item>
          <Item>0.21</Item>
          <Item>0.24</Item>
          <Item>0.27</Item>
          <Item>0.3</Item>
          <Item>0.5</Item>
        </mGOverPt>

        <ktG>
          <Item>0.0</Item>
          <Item>0.25</Item>
          <Item>0.5</Item>
          <Item>1.0</Item>
          <Item>1.5</Item>
          <Item>2.0</Item>
          <Item>3.0</Item>
          <Item>4.0</Item>
          <Item>6.0</Item>
          <Item>8.0</Item>
          <Item>10.0</Item>
          <Item>15.0</Item>
          <Item>20.0</Item>
          <Item>30.0</Item>
          <Item>50.0</Item>
        </ktG>

      </var>
    </SoftDropGroom>

  </observable>

  <inputFiles>
    <inputStyle>JetScapeAscii</inputStyle>
    <events>
      <head>JetscapeHadronListBin</head>
      <join>_</join>
      <tail>.out</tail>
    </events>
    <sigma>
      <path>na</path>
      <head>SigmaHardBin</head>
      <join>_</join>
      <tail>.out</tail>
    </sigma>
    <divNum>60</divNum>    <!-- 0: default, 1-:  divided-->
    <divUnit>_Run</divUnit>
  </inputFiles>

