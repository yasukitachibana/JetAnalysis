      <Item>100</Item>
      <Item>120</Item>
      <Item>150</Item>
      <Item>200</Item>
      <Item>300</Item>
      <Item>500</Item>
    </jetPtMax>

    <nJetEv>0</nJetEv>    <!-- 0: inclusive -->
  </jetReco>

  <!-- Set Particle Mass -->
