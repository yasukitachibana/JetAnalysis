<?xml version="1.0"?>

<jetAnalysis>

  <mode>1</mode>  <!-- 0: w/ combine, 1: w/o combine, 2: only combine-->
  <subtractionMethod>NaiveNegativeSub</subtractionMethod>

  <!-- Jet Reconstruction-->
  <jetReco>
    <reconstructionMethod>NegativeReco</reconstructionMethod>
   <jetR>
      <Item>0.001</Item>
      <Item>0.005</Item>
      <Item>0.01</Item>
      <Item>0.02</Item>
      <Item>0.03</Item>      
      <Item>0.04</Item>            
      <Item>0.05</Item>
      <Item>0.075</Item>      
      <Item>0.1</Item>
      <Item>0.2</Item>
      <Item>0.3</Item>
      <Item>0.4</Item>
      <Item>0.6</Item>
      <Item>0.8</Item>
      <Item>1.0</Item>
      <Item>1.2</Item>
      <Item>1.4</Item>
      <Item>1.5</Item>
    </jetR>
    <chJet>0</chJet>    <!-- 0: full jet, 1: charged jet -->
    <!-- <statJet>
      <Item>0</Item><Item>1</Item><Item>-1</Item>
    </statJet> -->

    <jetRapidity>1</jetRapidity>    <!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <jetRapMin>
      <Item>0</Item>
    </jetRapMin>
    <jetRapMax>
      <Item>5.0</Item>
    </jetRapMax>
    <jetPtMin>
      <Item>10</Item>
    </jetPtMin>
    <jetPtMax>
      <Item>10000</Item>
    </jetPtMax>
    <nJetEv>1</nJetEv>
  </jetReco>

  <observable>



    <!-- Hadrons -->

    <InconeQuant>
      <name>Incone Quantities</name>
      <K0SStrange>0</K0SStrange>
      <chParticle>0</chParticle>
      <!-- <statParticle>
        <Item>0</Item>
        <Item>1</Item>
        <Item>-1</Item>
      </statParticle> -->
      <particleRapidity>1</particleRapidity>
      <particleRapMin>
        <Item>0.0</Item>
      </particleRapMin>
      <particleRapMax>
        <Item>10.0</Item>
      </particleRapMax>
      <particlePtMin>
        <Item>0.0</Item>
      </particlePtMin>
      <particlePtMax>
        <Item>5210</Item>
      </particlePtMax>

      <var>
        <pT>
          <Item>0</Item>
          <Item>5</Item>
          <Item>10</Item>
          <Item>15</Item>          
          <Item>20</Item>
          <Item>30</Item>          
          <Item>40</Item>                    
          <Item>50</Item>
          <Item>60</Item>          
          <Item>70</Item>                    
          <Item>80</Item>                              
          <Item>90</Item>                                        
          <Item>100</Item>
          <Item>150</Item>
          <Item>200</Item>
          <Item>250</Item>
          <Item>300</Item>
          <Item>400</Item>
          <Item>500</Item>
          <Item>600</Item>
          <Item>800</Item>
          <Item>1000</Item>
        </pT>
      </var>
    </InconeQuant>


  </observable>

  <inputFiles>
    <inputStyle>JetScapeAscii</inputStyle>
    <events>
      <head>JetscapeHadronListBin</head>
      <join>_</join>
      <tail>.out</tail>
    </events>
    <sigma>
      <path>na</path>
      <head>SigmaHardBin</head>
      <join>_</join>
      <tail>.out</tail>
    </sigma>
    <divNum>0</divNum>    <!-- 0: default, 1-:  divided-->
    <divUnit>_Run</divUnit>
  </inputFiles>

  <!-- ptHatBins -->
  <ptHat>
    <Item>250</Item>
    <Item>260</Item>    
    <Item>300</Item>
    <Item>310</Item>    
    <Item>350</Item>
    <Item>360</Item>    
  </ptHat>


</jetAnalysis>
