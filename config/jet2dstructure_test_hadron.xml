<?xml version="1.0"?>

<jetAnalysis>

  <mode>0</mode>  <!-- 0: w/ combine, 1: w/o combine, 2: only combine-->
  <subtractionMethod>NaiveNegativeSub</subtractionMethod>

  <!-- Jet Reconstruction-->
  <jetReco>
    <reconstructionMethod>NegativeReco</reconstructionMethod>
    <jetR>
      <Item>0.4</Item>
    </jetR>
    <chJet>0</chJet>    <!-- 0: full jet, 1: charged jet -->
    <jetRapidity>1</jetRapidity>    <!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <jetRapMin>
      <Item>0</Item>
    </jetRapMin>
    <jetRapMax>
      <Item>1.6</Item>
    </jetRapMax>
    <jetPtMin>
      <Item>0</Item>
    </jetPtMin>
    <jetPtMax>
      <Item>5020</Item>
    </jetPtMax>
    <nJetEv>2</nJetEv>    <!-- 0: inclusive -->
  </jetReco>

  <observable>
    <JetShape2DNoMixedEvent>
      <name>Jet Shape Function [2D] w/o Mixed Event Method</name>

      <chParticle>0</chParticle>

      <!-- <statParticle>
        <Item>0</Item>
        <Item>1</Item>
        <Item>-1</Item>
      </statParticle> -->
      <particleRapidity>1</particleRapidity>

      <particleRapMin>
        <Item>0.0</Item>
      </particleRapMin>
      <particleRapMax>
        <Item>10</Item>
      </particleRapMax>

      <particlePtMin>
        <Item>0.0</Item>
      </particlePtMin>
      <particlePtMax>
        <Item>10000</Item>
      </particlePtMax>

      <var>
        <r>
          <Item>0</Item>
          <Item>0.05</Item>
          <Item>0.1</Item>
          <Item>0.15</Item>
          <Item>0.2</Item>
          <Item>0.25</Item>
          <Item>0.3</Item>
          <Item>0.35</Item>
          <Item>0.4</Item>
          <Item>0.45</Item>
          <Item>0.5</Item>
          <Item>0.6</Item>
          <Item>0.7</Item>
          <Item>0.8</Item>
          <Item>1.0</Item>
          <Item>1.2</Item>
          <Item>1.5</Item>
        </r>
      </var>

      <nBinPhi>63</nBinPhi>
      <boundPhi>
        <Item>-3.141592653589793238462643383279502884197</Item>
        <Item>3.141592653589793238462643383279502884197</Item>
      </boundPhi>
      <nBinEta>160</nBinEta>
      <boundEta>
        <Item>-8</Item>
        <Item>8</Item>
      </boundEta>

    </JetShape2DNoMixedEvent>

  </observable>

  <inputFiles>
    <inputStyle>JetScapeAscii</inputStyle>
    <events>
      <head>JetscapeHadronListBin</head>
      <join>_</join>
      <tail>.out</tail>
    </events>
    <sigma>
      <path>/wsu/home/go/go54/go5410/maj-shen/AAPaperData/5020_PP_Colorless</path>
      <head>SigmaHardBin</head>
      <join>_</join>
      <tail>.out</tail>
    </sigma>
    <divNum>0</divNum>    <!-- 0: default, 1-:  divided-->
    <divUnit>_Run</divUnit>
  </inputFiles>

  <!-- ptHatBins -->
  <ptHat>
    <Item>130</Item>
    <Item>140</Item>
  </ptHat>


</jetAnalysis>
