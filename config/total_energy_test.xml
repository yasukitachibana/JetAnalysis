<?xml version="1.0"?>

<jetAnalysis>
  
  <mode>0</mode><!-- 0: w/ combine, 1: w/o combine, 2: only combine-->
  <subtractionMethod>NaiveNegativeSub</subtractionMethod>
  
  <!-- Jet Reconstruction-->
  <jetReco>
    <reconstructionMethod>NegativeReco</reconstructionMethod>
    <jetR><Item>0.4</Item></jetR>
    <chJet>0</chJet><!-- 0: full jet, 1: charged jet -->
    <statJet>
      <Item>0</Item><Item>1</Item><Item>-1</Item>
    </statJet>
    <jetRapidity>1</jetRapidity><!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <jetRapMin><Item>0</Item></jetRapMin>
    <jetRapMax><Item>1.6</Item></jetRapMax>
    <jetPtMin><Item>0</Item></jetPtMin>
    <jetPtMax><Item>3000</Item></jetPtMax>
  </jetReco>
  
  <observable>
    
    
    
    <!-- Hadrons -->
    
    <TotalEnergy>
      <name>Total Energy</name>
      <chParticle>0</chParticle>
      <statParticle>
        <Item>0</Item><Item>1</Item><Item>-1</Item>
      </statParticle>
      <particleRapidity>1</particleRapidity>
      <particleRapMin><Item>0.0</Item></particleRapMin>
      <particleRapMax><Item>10000</Item></particleRapMax>
      <particlePtMin><Item>0.0</Item></particlePtMin>
      <particlePtMax><Item>10000</Item></particlePtMax>
      
      <var>
        <status>
          <Item>-1.5</Item>
          <Item>-0.5</Item>
          <Item>0.5</Item>
          <Item>1.5</Item>
        </status>
      </var>
      
    </TotalEnergy>
    
  </observable>
  
  <inputFiles>
    <inputStyle>JetScapeAscii</inputStyle>
    <events>
<!--      <head>SmallJet</head>-->
      <head>JetscapeHadronListBin</head>
      <join>_</join>
      <tail>.out</tail>
    </events>
    <sigma>
      <path>na</path>
      <head>SigmaHardBin</head>
      <join>_</join>
      <tail>.out</tail>
    </sigma>
    <divNum>0</divNum><!-- 0: default, 1-:  divided-->
    <divUnit>_Run</divUnit>
  </inputFiles>
  
