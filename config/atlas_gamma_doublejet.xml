<?xml version="1.0"?>

<jetAnalysis>

  <mode>0</mode>  <!-- 0: w/ combine, 1: w/o combine, 2: only combine-->
  <subtractionMethod>NaiveNegativeSub</subtractionMethod>

  <!-- Jet Reconstruction-->
  <jetReco>
    <reconstructionMethod>NegativeReco</reconstructionMethod>
    <jetR>
      <Item>0.2</Item>
    </jetR>
    <chJet>0</chJet>    <!-- 0: full jet, 1: charged jet -->


    <jetRapidity>1</jetRapidity>    <!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <jetRapMin>
      <Item>0</Item>
    </jetRapMin>
    <jetRapMax>
      <Item>2.8</Item>
    </jetRapMax>
    <jetPtMin>
      <Item>0</Item>
    </jetPtMin>
    <jetPtMax>
      <Item>1000.0</Item>
    </jetPtMax>
    <nJetEv>0</nJetEv>
  </jetReco>

  <jetTag>
    <tagged>1</tagged>
    <xJetTag>1</xJetTag>    <!-- 1: jet triggered by momentum fraction Xjtag instead of pTjet-->
    <nTagEv>0</nTagEv>    <!-- 0: inclusive, 1: Highest pt in each event -->
    <!-- tag basic iinfo -->
    <tagParticle>
      <Item>22</Item>
    </tagParticle>
    <tagStat>
      <Item>0</Item>
      <Item>1</Item>
    </tagStat>
    <!-- tag pt cut -->
    <tagPtMin>90</tagPtMin>
    <tagPtMax>180</tagPtMax>
    <!-- tag rapidity cut -->
    <tagRapidity>1</tagRapidity>    <!-- 0: rapidity(Y), 1: pseudorapidity(Eta) -->
    <tagRapMin>0</tagRapMin>
    <tagRapMax>2.37</tagRapMax>
    <!-- tag isolation -->
    <isolation>1</isolation>
    <RIsolation>0.3</RIsolation>
    <cutIsolation>3</cutIsolation>
    <varIsolation>ET</varIsolation>    <!-- pT, PT, Pt, pt: transverse momentum   ET, et, Et, eT: transverse energy -->
    <!-- <chIsolation>1</chIsolation>   -->
    <statIsolation>
      <Item>0</Item>
      <Item>1</Item>
      <Item>-1</Item>
    </statIsolation>
    <!-- tag relative angle -->
    <deltaPhiCut>1</deltaPhiCut>
    <deltaPhiMin>-0.875</deltaPhiMin>    <!-- negative value: to be multiplied by pi, 7/8 = 0.875-->
  </jetTag>

  <observable>



    <!-- Hadrons -->
    <TaggedJetPair>
      <name>Tagged Jet Pair</name>

      <!-- subjet infos -->
      <chParticle>0</chParticle>
      <particleRapidity>1</particleRapidity>
      <particleRapMin>        <!-- subjet! -->
        <Item>0.0</Item>
      </particleRapMin>
      <particleRapMax>        <!-- subjet! -->
        <Item>2.8</Item>
      </particleRapMax>
      <particlePtMin>        <!-- subjet! -->
        <Item>30.0</Item>
      </particlePtMin>
      <particlePtMax>        <!-- subjet! -->
        <Item>501</Item>
      </particlePtMax>

      <subjetDeltaPhiCut>1</subjetDeltaPhiCut>
      <subjetDeltaPhiMin>-0.5</subjetDeltaPhiMin>      <!--minimum relative angle between subjet and tag particle, negative value: to be multiplied by pi, 7/8 = 0.875-->

      <rBinEdges>
        <Item>0.4</Item>
        <Item>1000.0</Item>
      </rBinEdges>

      <aBinEdges>
        <Item>0.0</Item>
        <Item>1000.0</Item>
      </aBinEdges>

      <var>
        <xJJTag>
          <Item>0.0</Item>        
          <Item>0.15</Item>
          <Item>0.3</Item>
          <Item>0.45</Item>
          <Item>0.6</Item>
          <Item>0.75</Item>
          <Item>0.9</Item>
          <Item>1.05</Item>
          <Item>1.2</Item>
          <Item>1.5</Item>
          <Item>2.0</Item>
          <Item>5.0</Item>
          <Item>10.0</Item>                    
          <Item>15.0</Item>
          <Item>20.0</Item>                                                  
        </xJJTag>

        <aJJTag>
          <Item>0.0</Item>
          <Item>0.05</Item>
          <Item>0.1</Item>
          <Item>0.2</Item>
          <Item>0.3</Item>
          <Item>0.4</Item>
          <Item>0.6</Item>
          <Item>0.8</Item>
          <Item>1.0</Item>
          <Item>2.0</Item>
          <Item>5.0</Item>
          <Item>10.0</Item>
          <Item>15.0</Item>
          <Item>20.0</Item>                                                  
        </aJJTag>

        <rJJ>
          <Item>0.0</Item>
          <Item>0.1</Item>
          <Item>0.2</Item>
          <Item>0.3</Item>                              
          <Item>0.4</Item>
          <Item>0.5</Item>
          <Item>0.6</Item>
          <Item>0.8</Item>
          <Item>1.2</Item>
          <Item>1.6</Item>
          <Item>2.0</Item>
          <Item>3.0</Item>
          <Item>3.0</Item>
          <Item>5.0</Item>
          <Item>10.0</Item>
          <Item>20.0</Item>                                        
        </rJJ>
      </var>

    </TaggedJetPair>

  </observable>

  <inputFiles>
    <inputStyle>JetScapeAscii</inputStyle>
    <events>
      <head>JetscapeHadronListBin</head>
      <join>_</join>
      <tail>.out</tail>
    </events>
    <sigma>
      <path>na</path>
      <head>SigmaHardBin</head>
      <join>_</join>
      <tail>.out</tail>
    </sigma>
    <divNum>0</divNum>    <!-- 0: default, 1-:  divided-->
    <divUnit>_Run</divUnit>
  </inputFiles>

